public class SalaryRequest {
    private double askedPercent;

    public SalaryRequest(double percent) {
        this.askedPercent = percent;
    }

    public double getAskedPercent() {
        return askedPercent ;
    }

    public void setAskedPercent(double askedPercent) {
        this.askedPercent = askedPercent;
    }

}