package Classes;
abstract class Pokemon {
	
 void run(ContextPlayer cp) {}
 void fight(ContextPlayer cp) {}
 void rest(ContextPlayer cp) {}
 
 void setPoints (ContextPlayer cp, int points) {
    cp.setPoints(points);
 }
 void changeState (ContextPlayer cp, Pokemon p) {
    cp.changeState(p);
 }
 void endGame(ContextPlayer cp) {
	cp.endGame();
 }
}